{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:client": "vite dev --port 5000",
    "dev": "PORT=3000 NODE_ENV=development tsx src/index.ts",
    "build": "vite build && esbuild src/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/index.js",
    "build:frontend": "vite build",
    "build:backend": "esbuild src/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate:rds": "tsx scripts/migrate-rds.ts",
    "db:seed:plans": "tsx scripts/seed-subscription-plans.ts",
    "test": "node --max-old-space-size=4096 node_modules/.bin/jest",
    "test:watch": "node --max-old-space-size=4096 node_modules/.bin/jest --watch",
    "test:coverage": "node --max-old-space-size=4096 node_modules/.bin/jest --coverage",
    "test:frontend": "cd client && vitest",
    "test:frontend:coverage": "cd client && vitest --coverage",
    "test:frontend:ui": "cd client && vitest --ui",
    "test:e2e": "vitest run tests/e2e",
    "test:e2e:watch": "vitest tests/e2e",
    "test:all": "npm run test && npm run test:frontend && npm run test:e2e",
    "docs:generate": "swagger-jsdoc -d swagger.config.js src/routers/*.ts -o swagger.json",
    "docs:serve": "swagger-ui-serve swagger.json",
    "cdk:synth": "cd iac && npx cdk synth",
    "cdk:diff": "cd iac && npx cdk diff",
    "deploy:dev": "cd iac && STAGE=dev npx cdk deploy --all --require-approval never",
    "deploy:qa": "cd iac && STAGE=qa npx cdk deploy --all --require-approval never",
    "deploy:prod": "cd iac && STAGE=prod npx cdk deploy --all --require-approval never",
    "deploy:frontend:dev": "STAGE=dev ./scripts/deploy-frontend.sh",
    "deploy:frontend:qa": "STAGE=qa ./scripts/deploy-frontend.sh",
    "deploy:frontend:prod": "STAGE=prod ./scripts/deploy-frontend.sh",
    "build:frontend:only": "BUILD_ONLY=true ./scripts/deploy-frontend.sh"
  },
